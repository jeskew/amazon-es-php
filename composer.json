{
    "name": "jsq/amazon-es-php",
    "description": "Support for using IAM authentication with the official Elasticsearch PHP client",
    "keywords": ["elasticsearch", "aws", "search", "client", "iam"],
    "minimum-stability": "stable",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Jonathan Eskew",
            "email": "jonathan@jeskew.net"
        }
    ],
    "require": {
        "elasticsearch/elasticsearch": "^2.1|^5.0|^6.0",
        "aws/aws-sdk-php": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Aws\\ElasticsearchService\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aws\\ElasticsearchService\\": "tests"
        }
    }
}
